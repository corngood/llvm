
; RUN: llvm-upgrade < %s | llvm-as | llc -march=c > %t1.cbe.c
; RUN: gcc -B/usr/bin/ %t1.cbe.c -o %t1.cbe
; RUN: ./%t1.cbe

bool %doTest(ubyte %x) {
	%dec.0 = add ubyte %x, 255
    %tmp.1001 = trunc ubyte %dec.0 to bool
    ret bool %tmp.1001
}

int %main () {
    %result = call bool %doTest(ubyte 1)
    %p = cast bool %result to int
    ret int %p
}
