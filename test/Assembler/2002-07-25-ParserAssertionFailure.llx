; Make sure we don't get an assertion failure, even though this is a parse 
; error
; RUN: llvm-upgrade < %s | llvm-as -o /dev/null -f 2>&1 | grep 'No arguments'

%ty = type void (int)

declare %ty* %foo()

void %test() {
        call %ty* %foo( )               ; <%ty*>:0 [#uses=0]
        ret void
}

